<!--#set var="TITLE" value="500: Server error!"
--><!--#include virtual="include/top.html" -->
    <div id="container">
	<div id="inner">
	    <h1>ERROR 500</h1>
	    <p>The server encoutered an internal error and was unable to complete your request.</p>
<!--#if expr="-n v('REDIRECT_ERROR_NOTES')" -->
	    <p>Error message:<br /><!--#echo encoding="none" var="REDIRECT_ERROR_NOTES" --></p>
<!--#else -->
	    <p>This error may be related either to huge server load or some CGI script error.</p>
<!--#endif -->
	</div>
    </div>
<!--#include virtual="include/bottom.html" -->
