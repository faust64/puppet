OUTMAXCHANS_<%=trunk%> =<% if trunkhash['maxchans'] =~ /[0-9]/ -%> <%=trunkhash['maxchans']%><% end -%>

OUTFAIL_<%=trunk%> =<% if trunkhash['fail'] =~ /[a-z]/ -%> <%=trunkhash['fail']%><% end -%>

OUTPREFIX_<%=trunk%> =<% if trunkhash['prefix'] =~ /[0-9]/ -%> <%=trunkhash['prefix']%><% end -%>

OUTDISABLE_<%=trunk%> = <% if trunkhash['disable'] == true -%>on<% else -%>off<% end -%>

OUTKEEPCID_<%=trunk%> =<% if trunkhash['keepcid'] == "no" or trunkhash['keepcid'] == "off" -%> off<% end -%>

FORCEDOUTCID_<%=trunk%> =<% if trunkhash['forcecid'] =~ /[0-9]/ -%> <%=trunkhash['forcecid']%><% end -%>
<% end -%><% end -%>
