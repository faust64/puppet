class mysql::vars {
    $bind_addr           = lookup("mysql_bind_addr")
    $binlog_do_db        = lookup("mysql_binlog_do_db")
    $charset             = lookup("mysql_charset")
    $conf_dir            = lookup("mysql_conf_dir")
    $contact             = lookup("backup_contact")
    $expire_log_days     = lookup("mysql_expire_log_days")
    $include_dir         = lookup("mysql_include_dir")
    $keep_backup         = lookup("mysql_keep_backup")
    $key_buffer          = lookup("mysql_key_buffer")
    $lib_dir             = lookup("mysql_lib_dir")
    $log_slowqueries     = lookup("mysql_log_slowqueries")
    $max_allowed_packet  = lookup("mysql_max_allowed_packet")
    $max_binlog_size     = lookup("mysql_max_binlog_size")
    $max_connections     = lookup("mysql_max_connections")
    $munin_conf_dir      = lookup("munin_conf_dir")
    $munin_do_myisam_db  = lookup("mysql_munin_myisam_databases")
    $munin_innodb_probes = lookup("mysql_innodb_munin_probes")
    $munin_monitored     = lookup("mysql_munin")
    $munin_multi_probes  = lookup("mysql_multi_munin_probes")
    $munin_myisam_probes = lookup("mysql_myisam_munin_probes")
    $munin_probes        = lookup("mysql_munin_probes")
    $munin_service_name  = lookup("munin_node_service_name")
    $pid_file            = lookup("mysql_pid_file")
    $provider            = lookup("mysql_provider")
    $query_cache_limit   = lookup("mysql_query_cache_limit")
    $query_cache_size    = lookup("mysql_query_cache_size")
    $read_buffer_size    = lookup("mysql_read_buffer_size")
    $replicate_do_table  = lookup("mysql_replicate_do_table")
    $replicate_from      = lookup("mysql_replication_from")
    $repl_pass           = lookup("mysql_replication_passphrase")
    $repl_user           = lookup("mysql_replication_user")
    $rolling_backup      = lookup("mysql_rolling_backup")
    $rsyslog_conf_dir    = lookup("rsyslog_conf_dir")
    $rsyslog_srvname     = lookup("rsyslog_service_name")
    $rsyslog_version     = lookup("rsyslog_version")
    $runtime_group       = lookup("mysql_runtime_group")
    $runtime_user        = lookup("mysql_runtime_group")
    $service_name        = lookup("mysql_service_name")
    $server_id           = lookup("mysql_server_id")
    $slack_hook          = lookup("mysql_slack_hook_uri")
    $sock_dir            = lookup("mysql_sock_dir")
    $sock_file           = lookup("mysql_sock_file")
    $sort_buffer_size    = lookup("mysql_sort_buffer_size")
    $table_cache         = lookup("mysql_table_cache")
    $thread_cache_size   = lookup("mysql_thread_cache_size")
    $thread_concurrency  = lookup("mysql_thread_concurrency")
    $thread_stack        = lookup("mysql_thread_stack")

    if ($keep_backup != true) {
	$dumpdir = $keep_backup
    } else {
	$dumpdir = "/var/backups/mysql"
    }
}
