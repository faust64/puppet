class haproxy::vars {
    $backends             = lookup("haproxy_backends")
    $chroot_dir           = lookup("haproxy_chroot_dir")
    $conf_dir             = lookup("haproxy_conf_dir")
    $default_mode         = lookup("haproxy_default_mode")
    $default_nooptions    = lookup("haproxy_default_nooptions")
    $default_options      = lookup("haproxy_default_options")
    $default_ssl_verify   = lookup("haproxy_default_ssl_verify")
    $default_sticky       = lookup("haproxy_default_sticky")
    $default_timeouts     = lookup("haproxy_timeouts")
    $dh_size              = lookup("haproxy_dh_size")
    $do_letsencrypt       = lookup("apache_letsencrypt")
    $errors_dir           = lookup("haproxy_errors_dir")
    $front_acl            = lookup("haproxy_front_acl")
    $listen_check         = lookup("haproxy_listen")
    $logs_dir             = lookup("haproxy_logs_dir")
    $max_conn             = lookup("haproxy_max_conn")
    $munin_conf_dir       = lookup("munin_conf_dir")
    $munin_monitored      = lookup("haproxy_munin")
    $munin_probes         = lookup("haproxy_munin_probes")
    $munin_service_name   = lookup("munin_node_service_name")
    $nagios_runtime_user  = lookup("nagios_runtime_user")
    $peers                = lookup("haproxy_peers")
    $pki_master           = lookup("pki_master")
    $rsyslog_conf_dir     = lookup("rsyslog_conf_dir")
    $rsyslog_service_name = lookup("rsyslog_service_name")
    $runtime_group        = lookup("haproxy_runtime_group")
    $runtime_user         = lookup("haproxy_runtime_user")
    $service_name         = lookup("haproxy_service_name")
    $sni_rules            = lookup("haproxy_sni_rules")
    $spread_checks        = lookup("haproxy_spread_checks")
    $ssl_bind_ciphers     = lookup("haproxy_ssl_bind_ciphers")
    $ssl_bind_options     = lookup("haproxy_ssl_bind_options")
    $stats_listen         = lookup("haproxy_stats_listen")
    $stats_passphrase     = lookup("haproxy_stats_passphrase")
    $stats_port           = lookup("haproxy_stats_port")
    $stats_socket         = lookup("haproxy_stats_socket")
    $stats_timeout        = lookup("haproxy_stats_timeout")
    $stats_user           = lookup("haproxy_stats_user")
    $stick_table_size     = lookup("haproxy_stick_table_size")
    $sync_port            = lookup("haproxy_sync_port")
    $uptodate             = lookup("haproxy_uptodate")
    $with_collectd        = lookup("haproxy_collectd")

    if ($listen_check == false) {
	$listen = $ipaddress
    } else {
	$listen = $listen_check
    }
}
